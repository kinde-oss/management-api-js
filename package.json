    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.18",
    "vitest-fetch-mock": "^0.4.5"
  },
  "dependencies": {
    "@kinde/jwt-decoder": "^0.2.0",
    "aws-jwt-verify": "^5.0.0",
    "dotenv": "^17.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
